{
  "name": "dapp",
  "version": "0.1.0",
  "homepage": ".",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.1",
    "@walletconnect/web3-provider": "^1.6.6",
    "babel-loader": "8.1.0",
    "classnames": "^2.3.1",
    "env-cmd": "^10.1.0",
    "ethers": "^5.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "react-toastify": "^8.0.3",
    "redux-saga": "^1.1.3",
    "sass": "^1.43.3",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "use-saga-reducer": "^3.0.0",
    "web-vitals": "^1.0.1",
    "web3modal": "^1.9.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/i18next": "^13.0.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/numeral": "^2.0.2",
    "@types/truffle-privatekey-provider": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "eslint-plugin-typescript-sort-keys": "^1.8.0",
    "husky": "^7.0.4",
    "i18next-scanner": "^3.1.0",
    "lint-staged": "^12.3.1",
    "prettier": "^2.2.1",
    "truffle-privatekey-provider": "^1.5.0",
    "typescript": "4.3.5",
    "web3": "^1.6.1"
  },
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "eslint": "eslint './**/*.{js,jsx,ts,tsx}'",
    "eslint:fix": "yarn prettier && yarn eslint --fix",
    "prepare": "cd .. && husky install dapp/.husky",
    "prettier": "yarn prettier:js --loglevel silent && yarn prettier:ts --loglevel silent",
    "prettier:js": "yarn prettier:js:pure './**/*.{js,jsx}'",
    "prettier:js:pure": "prettier --config .prettierrc.yml --write",
    "prettier:ts": "yarn prettier:ts:pure './**/*.{ts,tsx}'",
    "prettier:ts:pure": "prettier --config .prettierrc.yml --parser typescript --write",
    "start": "react-scripts start",
    "test": "react-scripts test --watchAll=false",
    "type-check": "tsc --noEmit true"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">= 16.13.0 <17.0.0"
  },
  "lint-staged": {
    "./**/*.js": [
      "yarn prettier:js:pure",
      "eslint --fix"
    ],
    "./**/*.{ts,tsx}": [
      "yarn prettier:ts:pure",
      "eslint --fix"
    ]
  }
}
